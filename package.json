{
  "name": "calow",
  "version": "1.0.0",
  "description": "Intuitive minimalist calorie logging web app",
  "private": true,
  "scripts": {
    "start": "next",
    "build": "rm -rf .next && next build && rm -rf docs && mv out docs && touch docs/.nojekyll",
    "lint": "next lint",
    "test": "bun run test:types && bun run test:unit",
    "test:types": "tsc --noEmit",
    "test:unit": "bun test",
    "analyze": "ANALYZE=\"true\" next build"
  },
  "keywords": [
    "react",
    "nextjs",
    "calorie-app"
  ],
  "author": "bogas04@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.10.9",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@leeoniya/ufuzzy": "^1.0.19",
    "babel-plugin-react-compiler": "^1.0.0",
    "core-js": "^3.48.0",
    "focus-visible": "^5.2.1",
    "framer-motion": "^12.29.2",
    "fuse.js": "^7.1.0",
    "idb-keyval": "^6.2.2",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "swr": "^2.3.8",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.6",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18"
  },
  "prettier": {
    "printWidth": 120
  }
}
